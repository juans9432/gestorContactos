<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="540.0" prefWidth="850.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="co.edu.uniquindio.gestorcontactos.controladores.InicioControlador">

    <Label layoutX="27.0" layoutY="35.0" text="Nombre:" />
    <TextField fx:id="txtNombre" layoutX="100.0" layoutY="30.0" minWidth="270.0" />


    <Label layoutX="27.0" layoutY="85.0" text="Apellido:" />
    <TextField fx:id="txtApellido" layoutX="100.0" layoutY="80.0" minWidth="270.0" />


    <Label layoutX="27.0" layoutY="135.0" text="Numero:" />
    <TextField fx:id="txtNumero" layoutX="100.0" layoutY="130.0" minWidth="270.0" />


    <Label layoutX="27.0" layoutY="185.0" text="Correo:" />
    <TextField fx:id="txtCorreo" layoutX="100.0" layoutY="180.0" minWidth="270.0" />

    <Label layoutX="27.0" layoutY="235.0" text="Fecha de nacimiento:" />
    <DatePicker fx:id="datePickerFechaNacimiento" layoutX="150.0" layoutY="230.0" minWidth="270.0" />


    <Button fx:id="btnNuevoContacto" layoutX="150.0" layoutY="280.0" text="Crear nuevo contacto" />
    <TableView fx:id="tablaContactos" layoutX="408.0" layoutY="305.0" prefHeight="205.0" prefWidth="402.0">
        <columns>
            <TableColumn fx:id="colNombre" text="Nombre" />
            <TableColumn fx:id="colApellido" text="Apellido" />
            <TableColumn fx:id="colNumero" text="Numero" />
            <TableColumn fx:id="colCorreo" text="Correo electronico" />
            <TableColumn fx:id="colFecha" text="Fecha de nacimiento" />
        </columns>
    </TableView>

</AnchorPane>
